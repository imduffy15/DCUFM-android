<?xml version="1.0" encoding="UTF-8" ?><widget xmlns   = "http://www.w3.org/ns/widgets"        xmlns:gap   = "http://phonegap.com/ns/1.0"		id        = "com.dcufm.app"		versionCode="320"		version   = "0.0.1">		<name>DCUfm</name>		<description>Ireland's best student radio station.</description>		<author href="http://dcufm.com" email="dcufm@ianduffy.ie">			Ian Duffy		</author>        <gap:platform name="ios" />        <gap:platform name="android" />	    <content src="index.html" />	    <access origin="*" />        <preference name="prerendered-icon" value="false" />		<preference name="orientation" value="portrait" />	    <preference name="fullscreen" value="false" />	    <preference name="webviewbounce" value="false" />	    <preference name="UIWebViewBounce" value="false" />	    <preference name="DisallowOverscroll" value="true" />        <icon src="icon.png" />        <!-- iPhone / iPod Touch -->        <icon src="res/icon/ios/icon-57.png" gap:platform="ios" width="57" height="57" />        <icon src="res/icon/ios/icon-57-2x.png" gap:platform="ios" width="114" height="114" />        <!-- iPad -->        <icon src="res/icon/ios/icon-72.png" gap:platform="ios" width="72" height="72" />        <icon src="res/icon/ios/icon-72-2x.png" gap:platform="ios" width="144" height="144" />        <!-- Android -->        <icon src="res/icon/android/icon-36-ldpi.png" gap:platform="android" gap:qualifier="ldpi" />        <icon src="res/icon/android/icon-48-mdpi.png" gap:platform="android" gap:qualifier="mdpi" />        <icon src="res/icon/android/icon-72-hdpi.png" gap:platform="android" gap:qualifier="hdpi" />        <icon src="res/icon/android/icon-96-xhdpi.png" gap:platform="android" gap:qualifier="xhdpi" />        <gap:splash src="splash.png" />        <!-- iPhone and iPod touch -->        <gap:splash src="res/screen/ios/Default@2x~iphone.png" gap:platform="ios" width="320" height="480" />        <gap:splash src="res/screen/ios/Default~iphone.png" gap:platform="ios" width="640" height="960" />        <!-- iPhone 5 / iPod Touch (5th Generation) -->        <gap:splash src="res/screen/ios/Default-568h@2x~iphone.png" gap:platform="ios" width="640" height="1136" />        <!-- iPad -->        <gap:splash src="res/screen/ios/Default-Portrait~ipad.png" gap:platform="ios" width="768" height="1024" />        <!-- Retina iPad -->        <gap:splash src="res/screen/ios/Default-Portrait@2x~ipad.png" gap:platform="ios" width="1536" height="2048" />        <!-- Android -->        <gap:splash src="res/screen/android/screen-ldpi-portrait.png" gap:platform="android" gap:qualifier="ldpi" />        <gap:splash src="res/screen/android/screen-mdpi-portrait.png" gap:platform="android" gap:qualifier="mdpi" />        <gap:splash src="res/screen/android/screen-hdpi-portrait.png" gap:platform="android" gap:qualifier="hdpi" />        <gap:splash src="res/screen/android/screen-xhdpi-portrait.png" gap:platform="android" gap:qualifier="xhdpi" />	</widget>